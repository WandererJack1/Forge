{
  "name": "TheMill-router",
  "version": "0.19.4",
  "description": "React routing component for the framework TheMill. TheMill is the react/rust fullstack framework",
  "homepage": "https://TheMill.dev",
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build",
    "lint": "eslint .",
    "format": "prettier --check --ignore-unknown --ignore-path ../../.prettierignore .",
    "format:fix": "prettier --write --ignore-unknown --ignore-path ../../.prettierignore .",
    "typecheck": "tsc --noEmit",
    "test:watch": "vitest",
    "test": "vitest run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TheJackMan33/TheMill.git",
    "directory": "packages/TheMill-router"
  },
  "keywords": [],
  "author": "Valerio Ageno",
  "license": "MIT",
  "type": "module",
  "types": "dist/esm/index.d.ts",
  "main": "dist/esm/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "default": "./dist/esm/index.js"
    },
    "./package.json": "./package.json"
  },
  "peerDependencies": {
    "react": ">=19.0.0"
  },
  "dependencies": {
    "react-intersection-observer": "^9.13.0"
  },
  "devDependencies": {
    "@testing-library/react": "16.3.0",
    "@types/react": "19.1.1",
    "@vitejs/plugin-react-swc": "3.8.1",
    "happy-dom": "17.4.4",
    "react": "19.1.0",
    "vite": "6.1.3",
    "vite-config": "workspace:*",
    "vitest": "3.1.1"
  }
